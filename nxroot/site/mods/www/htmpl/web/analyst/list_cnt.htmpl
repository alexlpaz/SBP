<PDM_IF "${args.KEEP.IsProfileBrowser:}" == "1">
   hdrTitle = "Pesquisa de contato com perfil resumido";
   hdrTitleList = "Lista de contatos com perfil resumido";
<PDM_ELSE>
if ( groupOnly ) {
   hdrTitle = "Pesquisa de grupo";
   hdrTitleList = "Lista de grupos";
}
else if (assignee)
{
   hdrTitle = "Pesquisa de responsável"; //analyst and group
   hdrTitleList = "Lista de responsáveis";
}
else
   switch ("$args.KEEP.type.id") {
      case "2305": // Employee
         hdrTitle = "Pesquisa de funcionário";
         hdrTitleList = "Lista de funcionários";
         break;
      case "2307": // Analyst
         hdrTitle = "Pesquisa de analista";
         hdrTitleList = "Lista de analistas";
         break;
      case "2308": // Group
         hdrTitle = "Pesquisa de grupo";
         hdrTitleList = "Lista de grupos";
         break;
      case "2310": // Customer
         hdrTitle = "Pesquisa de cliente";
         hdrTitleList = "Lista de clientes";
         break;
      default: // Contacts
         hdrTitle = "Pesquisa de contato";
         hdrTitleList = "Lista de contatos";
         break;
   }
</PDM_IF>
function lstCntOnLoad()
{
