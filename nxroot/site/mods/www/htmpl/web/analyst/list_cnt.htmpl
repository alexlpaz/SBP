<script language="JavaScript">
var groupOnly = false;
var msgFoundNone, msgFound1, msgFound, msgFoundMore, msgSelect;
var searchFilterValue;
<PDM_IF "$args.KEEP.forResponse" == "1">
	searchFilterValue = "";
<PDM_ELSE>
	searchFilterValue = searchFilterValueOf("type", true);
</PDM_IF>
switch (searchFilterValue) {
   case "2305": // Employee
      msgFoundNone = "Nenhum funcionário encontrado";
      msgFound1    = "1 funcionário encontrado";
      msgFound     = "%1 funcionários encontrados";
      msgFoundMore = "%1 funcionários encontrados.  Exibindo de %2 a %3";
      msgSelect    = "Clique para selecionar o funcionário desejado";
      break;
   case "2307": // Analyst
      
<PDM_IF "$args.in_group_name" != "">
         msgFoundNone = "Nenhum analista encontrado no grupo, $args.in_group_name";
         msgFound1 = "1 analista encontrado no grupo, $args.in_group_name";
         msgFound = "%1 analista encontrado no grupo, $args.in_group_name";
         msgFoundMore = "%1 analistas encontrados no grupo, $args.in_group_name.  Exibindo de %2 a %3";
      
<PDM_ELSE>
         msgFoundNone = "Nenhum analista encontrado";
         msgFound1 = "1 analista encontrado";
         msgFound = "%1 analistas encontrados";
         msgFoundMore = "%1 analistas encontrados.  Exibindo de %2 a %3";
      
</PDM_IF>
      msgSelect = "Clique para selecionar o analista desejado";
      break;
   case "2308": // Group
      groupOnly = true;
      msgFoundNone = "Nenhum grupo encontrado";
      msgFound1    = "1 grupo encontrado";
      msgFound     = "%1 grupos encontrados";
      msgFoundMore = "%1 grupos localizados.  Exibindo de %2 a %3";
      msgSelect    = "Clique para selecionar o grupo desejado";
      break;
   case "2310": // Customer
      msgFoundNone = "Nenhum cliente encontrado";
      msgFound1    = "1 cliente encontrado";
      msgFound     = "%1 clientes encontrados";
      msgFoundMore = "%1 clientes encontrados.  Exibindo de %2 a %3";
      msgSelect    = "Clique para selecionar o cliente desejado";
      break;
   default: // Contacts
      if ("$args.KEEP.AgtGrp" == "1")
      {
         msgFoundNone = "Nenhum responsável encontrado";
         msgFound1    = "1 responsável encontrado";
         msgFound     = "%1 responsáveis encontrados";
         msgFoundMore = "%1 responsáveis encontrados.  Exibindo de %2 a %3";
         msgSelect    = "Clique para selecionar o responsável desejado";
      }
      else
      {
         msgFoundNone = "Nenhum contato encontrado";
         msgFound1    = "1 contato encontrado";
         msgFound     = "%1 contatos encontrados";
         msgFoundMore = "%1 contato(s) encontrado(s).  Exibindo de %2 a %3";
         msgSelect    = "Clique para selecionar o contato desejado";
      }
      break;
}
function showLoc(value)
{
	var lochtml;
	lochtml = "<A CLASS=record HREF=\"javascript:showDetailWithPersid('" + loc_persid + "')\">" + value + "</A>" ;
	rs.data(lochtml);
}
function showEmail(value)
{
    var emailhtml = "";
    if (value != "") {
        emailhtml = "<a class='record' href=\"mailto:" + value + "\">" + value + "</a>" ;
    }
    rs.data(emailhtml);
}

