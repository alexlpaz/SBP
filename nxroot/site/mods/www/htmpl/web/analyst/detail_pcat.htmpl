var hdrTitleUpd = <PDM_FMT PAD=NO ESC_STYLE=C>"$args.sym - Atualizar área de solicitação/incidente/problema"</PDM_FMT>;
var hdrTitleNew = <PDM_FMT PAD=NO ESC_STYLE=C>"Criar área de solicitação/incidente/problema $args.sym"</PDM_FMT>;
</script>
<PDM_INCLUDE FILE=std_head.htmpl>
<SCRIPT LANGUAGE="JavaScript">
