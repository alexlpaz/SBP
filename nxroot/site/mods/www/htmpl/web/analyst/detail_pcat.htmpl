<PDM_MACRO name=dtlLookup hdr="Grupo" attr=group>
</PDM_IF>
<PDM_MACRO name=dtlLookup hdr="Responsável" attr=assignee>
<PDM_IF $args.owning_contract \> 0>
	<!-- Search only for Service Types belonging to the category's Svc Contract -->
	
<PDM_MACRO name=dtlLookup hdr="Tipo de serviço" attr=service_type extraurl="ADDITIONAL_WHERE=owning_contract=$args.owning_contract+KEEP.SVC_CONTRACT_ID=$args.owning_contract+KEEP.SVC_CONTRACT_SYM=$args.owning_contract.sym">
<PDM_ELSE>
	<!-- Search only service types that are public, with no service contract -->
	
<PDM_MACRO name=dtlLookup hdr="Tipo de serviço" attr=service_type extraurl="ADDITIONAL_WHERE=owning_contract IS NULL+KEEP.SVC_CONTRACT_ID=0+KEEP.SVC_CONTRACT_SYM=(empty)">
</PDM_IF>
<PDM_MACRO name=dtlStartRow>
<PDM_IF "$env.NX_SAP_GUID" != "">
