<PDM_ELSE>
<PDM_MACRO name=dtlLookup hdr="Pesquisa" attr=survey>
</PDM_IF>
<PDM_IF "$prop.form_name_3" == "edit">
<PDM_MACRO name=dtlReadonly hdr="Contrato de serviço" attr=owning_contract colspan=1>
<PDM_ELSE>
<PDM_MACRO name=dtlLookup hdr="Contrato de serviço" attr=owning_contract colspan=1>
</PDM_IF>
<PDM_MACRO name=dtlDropdown hdr="Urgência da área" attr="category_urgency" size=20>
<PDM_MACRO name=dtlStartRow>
<PDM_IF "$prop.form_name_3" == "edit" >
<PDM_MACRO name=dtlCheckbox hdr="Inclusão de autoatendimento" attr=ss_include evt="onClick='ss_include_click(this)'" off=Não on=Sim>
<PDM_MACRO name=dtlTextbox hdr="Símbolo de autoatendimento" attr=ss_sym colspan=2 make_required=yes>
    ss_sym_fldNum = _dtl.fieldToValidate.length - 1;

    
<PDM_MACRO name=dtlStartRow>
    if ( typeof ss_sym_fld == "undefined" ) {
        ss_sym_fld = findFieldAndLabelFromTable("ss_sym",1);
    }
    if ("$args.ss_include" == "1") {
        if ( ss_sym_fld != null ) {
            _dtl.fieldToValidate[ss_sym_fldNum].is_required = 1;
            ss_sym_fld[0].style.display = "block";
            ss_sym_fld[1].style.display = "block";
        }
    }
    else {
        if ( ss_sym_fld != null ) {
            _dtl.fieldToValidate[ss_sym_fldNum].is_required = 0;
            ss_sym_fld[0].style.display = "none";
            ss_sym_fld[1].style.display = "none";
        }
    }
<PDM_ELSE>
<PDM_MACRO name=dtlCheckboxReadonly hdr="Inclusão de autoatendimento" attr="ss_include" off=Não on=Sim>
<PDM_MACRO name=dtlReadonly hdr="Símbolo de autoatendimento" attr="ss_sym" colspan=1>
</PDM_IF>
<PDM_MACRO name=dtlLookup hdr="Serviço" attr=z_srl_servico colspan=1>
